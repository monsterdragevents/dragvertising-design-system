{
  "$schema": "./component-mapping-schema.json",
  "version": "2.0.0",
  "description": "1:1 mapping between Figma components and React components using consistent naming",
  "namingConvention": {
    "figma": "cmp / [category] / [component-name]",
    "react": "src/components/[category]/[ComponentName].tsx"
  },
  "mappings": [
    {
      "figmaComponent": "cmp / button / primary",
      "reactComponent": "src/components/button/PrimaryButton.tsx",
      "componentName": "PrimaryButton",
      "props": {
        "size": "sm | md | lg",
        "state": "default | hover | pressed | disabled"
      },
      "notes": "Primary action button with size and state variants"
    },
    {
      "figmaComponent": "cmp / button / secondary",
      "reactComponent": "src/components/button/SecondaryButton.tsx",
      "componentName": "SecondaryButton",
      "props": {
        "size": "sm | md | lg"
      },
      "notes": "Secondary action button"
    },
    {
      "figmaComponent": "cmp / card / talent-browse",
      "reactComponent": "src/components/card/TalentBrowseCard.tsx",
      "componentName": "TalentBrowseCard",
      "props": {
        "state": "default | hover | loading | empty"
      },
      "notes": "Browse card for talent profiles - includes avatar, name, pronouns, bio"
    },
    {
      "figmaComponent": "cmp / card / dj-browse",
      "reactComponent": "src/components/card/DJBrowseCard.tsx",
      "componentName": "DJBrowseCard",
      "props": {
        "state": "default | hover | loading | empty"
      },
      "notes": "Browse card for DJ profiles - golden accent theme"
    },
    {
      "figmaComponent": "cmp / card / show-preview",
      "reactComponent": "src/components/card/ShowPreviewCard.tsx",
      "componentName": "ShowPreviewCard",
      "props": {
        "variant": "default | featured"
      },
      "notes": "Show preview card for browse and listings"
    },
    {
      "figmaComponent": "cmp / card / venue-preview",
      "reactComponent": "src/components/card/VenuePreviewCard.tsx",
      "componentName": "VenuePreviewCard",
      "props": {
        "variant": "default | compact"
      },
      "notes": "Venue preview card with location info"
    },
    {
      "figmaComponent": "cmp / nav / universe-switcher",
      "reactComponent": "src/components/nav/UniverseSwitcher.tsx",
      "componentName": "UniverseSwitcher",
      "props": {
        "placement": "header | sidebar"
      },
      "notes": "Universe switching dropdown - adapts to placement"
    },
    {
      "figmaComponent": "cmp / nav / main-nav",
      "reactComponent": "src/components/nav/MainNav.tsx",
      "componentName": "MainNav",
      "props": {
        "variant": "public | authenticated"
      },
      "notes": "Main navigation bar"
    },
    {
      "figmaComponent": "cmp / nav / sidebar",
      "reactComponent": "src/components/nav/Sidebar.tsx",
      "componentName": "Sidebar",
      "props": {
        "collapsed": "boolean"
      },
      "notes": "Dashboard sidebar navigation"
    },
    {
      "figmaComponent": "cmp / layout / dashboard-shell",
      "reactComponent": "src/components/layout/DashboardShell.tsx",
      "componentName": "DashboardShell",
      "props": {
        "universe": "talent | dj | producer | fan"
      },
      "notes": "Main dashboard layout shell - universe-aware"
    },
    {
      "figmaComponent": "cmp / layout / page-header",
      "reactComponent": "src/components/layout/PageHeader.tsx",
      "componentName": "PageHeader",
      "props": {
        "showBreadcrumbs": "boolean"
      },
      "notes": "Page header with title and optional breadcrumbs"
    },
    {
      "figmaComponent": "cmp / form / input-text",
      "reactComponent": "src/components/form/InputText.tsx",
      "componentName": "InputText",
      "props": {
        "state": "default | focus | error | disabled"
      },
      "notes": "Text input field"
    },
    {
      "figmaComponent": "cmp / form / select",
      "reactComponent": "src/components/form/Select.tsx",
      "componentName": "Select",
      "props": {
        "state": "default | open | disabled"
      },
      "notes": "Select dropdown"
    },
    {
      "figmaComponent": "cmp / modal / dialog",
      "reactComponent": "src/components/modal/Dialog.tsx",
      "componentName": "Dialog",
      "props": {
        "size": "sm | md | lg | xl"
      },
      "notes": "Modal dialog - uses shadcn/ui Dialog primitive"
    },
    {
      "figmaComponent": "cmp / feedback / toast",
      "reactComponent": "src/components/feedback/Toast.tsx",
      "componentName": "Toast",
      "props": {
        "variant": "default | success | error | warning"
      },
      "notes": "Toast notification - uses sonner"
    }
  ],
  "categories": [
    "button",
    "card",
    "nav",
    "layout",
    "form",
    "modal",
    "feedback"
  ],
  "variantMapping": {
    "description": "Figma variants map directly to React props",
    "examples": [
      {
        "figmaVariant": "state=default",
        "reactProp": "state=\"default\""
      },
      {
        "figmaVariant": "size=md",
        "reactProp": "size=\"md\""
      },
      {
        "figmaVariant": "variant=primary",
        "reactProp": "variant=\"primary\""
      }
    ]
  },
  "legacyMappings": [
    {
      "figmaComponent": "Button/Secondary",
      "reactComponent": "src/ui/primitives/button.tsx",
      "componentName": "Button",
      "props": {
        "variant": "secondary"
      },
      "notes": "Secondary action button"
    },
    {
      "figmaComponent": "Button/Destructive",
      "reactComponent": "src/ui/primitives/button.tsx",
      "componentName": "Button",
      "props": {
        "variant": "destructive"
      },
      "notes": "Destructive action button (delete, remove)"
    },
    {
      "figmaComponent": "Button/Outline",
      "reactComponent": "src/ui/primitives/button.tsx",
      "componentName": "Button",
      "props": {
        "variant": "outline"
      },
      "notes": "Outline button"
    },
    {
      "figmaComponent": "Button/Ghost",
      "reactComponent": "src/ui/primitives/button.tsx",
      "componentName": "Button",
      "props": {
        "variant": "ghost"
      },
      "notes": "Ghost button (minimal)"
    },
    {
      "figmaComponent": "Input/Text",
      "reactComponent": "src/ui/primitives/input.tsx",
      "componentName": "Input",
      "props": {
        "type": "text"
      },
      "notes": "Text input field"
    },
    {
      "figmaComponent": "Input/Email",
      "reactComponent": "src/ui/primitives/input.tsx",
      "componentName": "Input",
